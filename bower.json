{
  "name": "piece-bower",
  "version": "0.0.1",
  "dependencies": {
    "requirejs": "*",
    "backbone": "*",
    "underscore": "*",
    "fastclick": "*",
    "zepto": "*",
    "iscroll": "*",
    "requirejs-text": "*",
    "requirejs-i18n": "*",
    "requirejs-domready": "*",

    "ratchet": "*"
  },
  "exportsOverride": {
    "requirejs": {
      "js": "require.js"
    },
    "backbone": {
      "js": "backbone.js"
    },
    "underscore": {
      "js": "underscore.js"
    },
    "fastclick": {
      "js": "lib/fastclick.js"
    },
    "zepto": {
      "js": "zepto.js"
    },
    "iscroll": {
      "js": "build/iscroll.js"
    },
    "requirejs-text": {
      "js": "text.js"
    },
    "requirejs-i18n": {
      "js": "i18n.js"
    },
    "requirejs-domready": {
      "js": "domready.js"
    },

    "ratchet": {
      "js": "dist/*.js",
      "css": "dist/*.css",
      "html": "dist/*.html"
    },

    "*": {
      "js": "**/*.js",
      "css": "**/*.css"
    }
  }
}