{
  "name": "piece-bower",
  "version": "0.0.1",
  "dependencies": {
    "almond": "*",
    "backbone": "*",
    "underscore": "*",
    "zepto": "*",
    "iscroll": "*",
    "requirejs-text": "*",
    "requirejs-i18n": "*",
    "requirejs-domready": "*",

    "ratchet": "*"
  },
  "exportsOverride": {
    "almond": {
      "js": "almond.js"
    },
    "backbone": {
      "js": "backbone.js"
    },
    "underscore": {
      "js": "underscore.js"
    },
    "zepto": {
      "js": "zepto.js"
    },
    "iscroll": {
      "js": "build/iscroll.js"
    },
    "requirejs-text": {
      "js": "text.js"
    },
    "requirejs-i18n": {
      "js": "i18n.js"
    },
    "requirejs-domready": {
      "js": "domready.js"
    },

    "ratchet": {
      "js": "dist/*.js",
      "css": "dist/*.css",
      "html": "dist/*.html"
    },

    "*": {
      "js": "**/*.js",
      "css": "**/*.css"
    }
  }
}